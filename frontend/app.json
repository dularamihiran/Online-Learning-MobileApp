{
  "expo": {
    "name": "Learnova",
    "slug": "learnova",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "learnova",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.learnova.app"
    },
    "android": {
      "package": "com.learnova.app",
      "adaptiveIcon": {
        "backgroundColor": "#3498db"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
